void setup() 
{
  for(int i = 1; i <=12;i++)
  {
    pinMode(i, INPUT);
  }
  Serial.begin(9600);
}
int x;
void loop() {
char output;
      
       
        for(int i = 2; i<= 7;i++)
        {
         if(!digitalRead(i))
          {
            for(int v = 0;v<2;v++)
                      {
                        for(int z = 0;z<3;z++)
                        {
                          Serial.println(temp[v][z]);
                        }
                      }
             if(i==2)
             {
               temp[0][0] = 1;
             }
             if(i==3)
             {
               temp[0][1] = 1;
             }
             if(i==4)
             {
               temp[0][2] = 1;
               }
             if(i==5)
             {
                temp[1][0] = 1;
             }
              if(i==6)
              {
                temp[1][1] = 1;
              }
              if(i==7)
              {
                 for(int n = 0; n<25; n++)
                  {
                    if(equal(temp,alpha[n])==1)
                    {
                      output = abc[n];
                      l+=output;
                      Serial.print(l);
                      l="";
                      output = ' ';
                      for(int c = 0;c<2;c++)
                      {
                        for(int e = 0;e<3;e++)
                        {
                          temp[c][e] = 0;
                        }
                      }
                    }
                  }                  
              }
            /*  if(i==8)
              {
                  Serial.print(i);
              }*/

}

          /*  if(i=7)
            {
              temp[2][1] = 1;
            }*/
          }
     delay(100);    
      }
    
