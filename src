bool temp[2][3] = {{0,0,0},{0,0,0}};
int z = -1;
String l = " ";


  
bool equal (bool tem1[2][3], bool tem2[2][3])
{
  for(int k = 0;k<2;k++)
  {
      for(int j = 0;j<3;j++)
      {
          if(tem1[k][j] != tem2[k][j])
          {
              return 0;
          }
      }
    }
    return 1;    
} 

